CREATE TABLE IF NOT EXISTS {dsbxx_}monitoruserdetails 
 (
	monUserID int(10) unsigned NOT NULL auto_increment,
	monUserReferer varchar(255) {gssb_charset} default NULL,
	monUserTimestamp timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
	monUserBrowser_name_pattern varchar(255) {gssb_charset} default NULL,
	monUserBrowser varchar(85) {gssb_charset} default NULL,
	monUserVersion varchar(85) {gssb_charset} default NULL,
	monUserPlatform varchar(155) {gssb_charset} default NULL,
	PRIMARY KEY  (monUserID)
 ) {gssb_charset};