CREATE TABLE IF NOT EXISTS {dsbxx_}monitorpageviews 
 (
	monPageID int(10) unsigned NOT NULL auto_increment,
	monPageName varchar(45) {gssb_charset} NOT NULL default '',
	monPageVisitTimestamp timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
	monPageApplicID varchar(45) {gssb_charset} NOT NULL default '',
	PRIMARY KEY  (monPageID)
 ) {gssb_charset};